{
  "hosting": {
    "public": "storybook-static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*.html",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "private, max-age=0, no-cache"
      } ]
    } ]
  }
}
